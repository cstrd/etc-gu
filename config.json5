{
  // where is your cloudformation stack running?
  aws: {
    region: 'us-east-1',
    profile: 'media-service',
    stackName: 'media-service-DEV'
  },

  // Domain root of your site, e.g. foobar.co.uk
  domainRoot: 'grid.local',

  // An API key used to ingest images from the s3watcher.
  s3Watcher: {
    key: ''
  },

  es6: {
    cluster: 'media-service',
    url: 'http://localhost:9200',
    shards: 1,
    replicas: 0
  },

  // Guardian specific properties for the Usage service
  capi: {
    poll:  5,
    live: {
      url: '',
      key: ''
    }
  },

  crier: {
    live: {
      streamName: '',
      roleArn: ''
    },
    preview: {
      streamName: '',
      roleArn: ''
    }
  },

  composer: {
    url: ''
  },

  google: {
    tracking: {
      id: ''
    }
  }
}
